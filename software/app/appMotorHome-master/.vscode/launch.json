  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch development",
      "request": "launch",
      "type": "dart",
      "program": "lib/main_development.dart",
      "args": [
        "--flavor",
        "development",
        "--target",
        "lib/main_development.dart"
      ],
      "toolArgs": [
        "--dart-define",
        "BLUETOOTH=antural",
        "--dart-define",
        "BROKER=190.220.31.11"
        
      ]
    },
    {
      "name": "Launch staging",
      "request": "launch",
      "type": "dart",
      "program": "lib/main_staging.dart",
      "args": ["--flavor", "staging", "--target", "lib/main_staging.dart"],
      "toolArgs": [
        "--dart-define",
        "BLUETOOTH=antural",
        "--dart-define",
        "BROKER=190.220.31.11"
        
      ]
    },
    {
      "name": "Launch production",
      "request": "launch",
      "type": "dart",
      "program": "lib/main_production.dart",
      "args": [
        "--flavor",
        "production",
        "--target",
        "lib/main_production.dart"
      ],
      "toolArgs": [
        "--dart-define",
        "BLUETOOTH=antural",
        "--dart-define",
        "BROKER=190.220.31.11"
        
      ]
    }
  ]
}
